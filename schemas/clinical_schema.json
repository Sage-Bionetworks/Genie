{
    "$schema": "https://json-schema.org/draft-07/schema",
    "$id": "https://example.com/product.schema.json",
    "title": "Clinical Schema",
    "description": "Clinical json schema",
    "type": "array",
    "minItems": 1,
    "items": {
        "properties": {
            "PATIENT_ID": {
                "description": "The unique patient identifier for the GENIE project.",
                "type": "string",
                "pattern": "^GENIE-(MSK|NKI)-.+"
            },
            "SEX": {
                "description": "The sex code for the patient",
                "type": "integer",
                "enum": [-1, 1, 2, 3, 4, 5, 6, 9]
            },
            "PRIMARY_RACE": {
                "description": "The primary race recorded for the patient",
                "type": "integer",
                "enum": [
                    1, 2, 3, 4, 5, 6, 7, 8, 10, 11, 12, 13, 14,
                    15, 16, 17, 20, 21, 22, 25, 26, 27, 28, 30, 31,
                    32, 88, 96, 97, 98, 99, -1
                ]
            },
            "SECONDARY_RACE": {
                "description": "The second race recorded for the patient",
                "type": "integer",
                "enum": [
                    1, 2, 3, 4, 5, 6, 7, 8, 10, 11, 12, 13, 14,
                    15, 16, 17, 20, 21, 22, 25, 26, 27, 28, 30, 31,
                    32, 88, 96, 97, 98, 99, -1
                ]
            },
            "TERTIARY_RACE": {
                "description": "The third race code for the patient",
                "type": "integer",
                "enum": [
                    1, 2, 3, 4, 5, 6, 7, 8, 10, 11, 12, 13, 14,
                    15, 16, 17, 20, 21, 22, 25, 26, 27, 28, 30, 31,
                    32, 88, 96, 97, 98, 99, -1
                ]
            },
            "ETHNICITY": {
                "description": "Indication of Spanish/Hispanic origin of the patient",
                "type": "integer",
                "minimum": -1,
                "maximum": 9
            },
            "BIRTH_YEAR": {
                "description": "The four digit year associated with the patient's date of birth",
                "type": "string",
                "pattern": "^(19|20)\\d{2}$|^>89$|^<18$|^Unknown$"
            },
            "YEAR_CONTACT": {
                "description": "Record the year that the patient is last known to be alive - data to be derived from information including and EHR, tumor registry, or other system",
                "type": "string",
                "pattern": "^(19|20)\\d{2}$|^>89$|^<18$|^Not Collected$|^Unknown$"
            },
            "YEAR_DEATH": {
                "description": "Year of death",
                "type": "string",
                "pattern": "^(19|20)\\d{2}$|^>89$|^<18$|^Not Collected$|^Unknown$|^Not Applicable$"
            },
            "INT_CONTACT": {
                "description": "Interval in days from DOB to date of last contact",
                "type": "string",
                "pattern": "^[\\d]{4,5}$|^Not Collected$|^Unknown$|>32485|<6570"
            },
            "INT_DOD": {
                "description": "Interval in days from DOB to DOD.",
                "type": "string",
                "pattern": "^[\\d]{4,5}$|^Not Collected$|^Unknown$|^Not Applicable$|>32485|<6570"
            },
            "DEAD": {
                "description": "Is this patient known to be deceased",
                "type": "string",
                "pattern": "^True$|^Not Collected$|^Unknown$|^False$|^Not Applicable$"
            }
        },
        "required": [
            "PATIENT_ID",
            "SEX",
            "BIRTH_YEAR",
            "YEAR_CONTACT",
            "YEAR_DEATH",
            "INT_CONTACT",
            "INT_DOD",
            "DEAD"
        ]
    }
}